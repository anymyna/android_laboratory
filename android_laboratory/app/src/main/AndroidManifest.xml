<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.ui">


    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.CAMERA" />

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <uses-permission android:name="android.permission.READ_SMS" />



    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:appComponentFactory"
        android:appComponentFactory="ComponentFactory"
        >

        <activity android:name=".HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".rxjava.RxBindingActivity"
            android:label="RxBinding"
            >
        </activity>

        <activity
            android:name=".TabLayoutTopActivity"
            android:label="TabLayout"
            android:screenOrientation="portrait"/>

        <activity
            android:name=".SubsamplingActivity"
            android:label="Subsampling"
            android:screenOrientation="portrait"/>


        <activity
            android:name=".rxjava.RxJavaActivity"
            android:label="RxJava"
            android:screenOrientation="portrait"/>

        <activity
            android:name=".OkhttpDemoActivity"
            android:label="Okhttp"
            android:screenOrientation="portrait"/>


        <activity
            android:name=".AddFlutterViewActivity"
            android:label="FlutterView"
            android:screenOrientation="portrait"/>


        <activity
            android:name=".rxjava.RxPermissionsActivity"
            android:label="RxPermissions"
            android:screenOrientation="portrait"/>


        <activity
            android:name=".EasyPermissionsActivity"
            android:label="EasyPermissions"
            android:screenOrientation="portrait"/>



        <activity
            android:name=".ParallaxBackLayoutActivity"
            android:label="ParallaxBackLayout"
            android:screenOrientation="portrait"/>


        <activity
            android:name=".DBFlowDemoActivity"
            android:label="DB"
            android:screenOrientation="portrait"/>


        <activity
            android:name=".PhotoViewActivity"
            android:label="PhotoView"
            android:screenOrientation="portrait"/>


        <activity
            android:name=".retrofit.RetrofitGetRequestActivity"
            android:label="Retrofit"
            android:screenOrientation="portrait"/>

        <activity
            android:name=".retrofit.RetrofitPostRequestActivity"
            android:label="Retrofit"
            android:screenOrientation="portrait"/>

    </application>



</manifest>